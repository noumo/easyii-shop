{
    "name": "noumo/easyii-start",
    "description": "Easy CMS based on Yii2 Framework",
    "keywords": ["yii2", "cms", "control panel", "admin", "easy"],
    "homepage": "http://easyiicms.com",
    "type": "project",
    "license": "GNU GPL V3",
    "support": {
        "wiki": "http://easyiicms.com/docs",
        "source": "https://github.com/noumo/easyii.git",
        "issues": "https://github.com/noumo/easyii/issues"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "noumo/easyii": "dev-test",
        "2amigos/yii2-date-time-picker-widget" : "~1.0",
        "bower-asset/eonasdan-bootstrap-datetimepicker": "^4.7@dev",
        "bower-asset/bootstrap-tagsinput": "^0.5.0@dev",
        "creocoder/yii2-taggable": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "generateCookieValidationKey": [
                "app/config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
	    "writable": [
            "assets",
            "uploads",
            "runtime"
        ]
    }
}
